@{

    ViewData["Title"] = "Bienvenido";
}

<h1>Bienvenido</h1>

<ul>
    @{
        int numVeces = (int)ViewData["numVeces"];
    
        for (int i = 1; i <= numVeces; i++){
            
            <li>@ViewData["nombre"] @ViewData["apellido"], veces mostrado = @i</li>

        }
    }
</ul>